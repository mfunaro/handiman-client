{{#simple-modal title='Settings' ok='addRound' close='removeModal' target=this}}
  <form {{action 'ok' on='submit'}}>
    <div class="form-group">
      <label>Score</label>
      {{input class="form-control" value=model.score}}
    </div>

    <div class="form-group">
      <label>Holes</label>
      {{input class="form-control" value=model.holes}}
    </div>
  </form>
{{/simple-modal}}
